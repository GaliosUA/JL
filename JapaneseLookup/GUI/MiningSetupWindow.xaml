<Window x:Class="JapaneseLookup.GUI.MiningSetupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MiningSetupWindow" Height="450" Width="800">
    <Grid Margin="10">
        <StackPanel Margin="10">
            <ComboBox x:Name="ComboBoxDeckNames" />
            <Button x:Name="ButtonRefresh" Click="ButtonRefresh_Click">Refresh</Button>
            <ComboBox x:Name="ComboBoxModelNames" />

            <Button x:Name="ButtonGetFields" Click="ButtonGetFields_Click">Get Fields</Button>
            <StackPanel x:Name="StackPanelFields" Margin="10" />

            <Button x:Name="ButtonSave" Click="ButtonSave_Click">Save</Button>
        </StackPanel>
    </Grid>
</Window>